#include <stdio.h>
#include <string.h>
#include<iostream>
using namespace std;
 
int main()
{
    string str;  
    int i,n,c=0;
    getline(cin,str);
    n=str.length();
    
 
    for(i=0;i<n/2;i++)  
    {
    	if(str[i]==str[n-i-1])
    	c++;
 
 	}
 	if(c==i)
 	    cout<<"pallindrome";
    else
       cout<<"Not pallindrome";
 
 	 
     
    return 0;
}
