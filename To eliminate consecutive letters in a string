#include<conio.h>
#include<iostream>
using namespace std;
main()
{
	int i,j;
	string str,str1;
	cout<<"enter string";
	cin>>str;
	str1[0]=str[0];
	cout<<str1[0];
	
	for(i=0;i<str.length()-1;i=i+1)
	{
		if(str1[i]!=str[i+1])
		{
			str1[i+1]=str[i+1];
			cout<<str[i+1];
		}
		else
		str1[i+1]=str[i];
		
	}

}
