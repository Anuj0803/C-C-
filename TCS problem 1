#include<iostream>
#include<conio.h>
using namespace std;
main()
{
	int i,j,k=0;
	string str[20]={"case", "break", "continue", "default", "defer", "else", "for", "func", "goto", "if", "map", "range", "return", "struct", "type", "var"};
	string str1;
	cout<<"enter string";
	cin>>str1;
	for(i=0;i<20;i++)
	{
	  if(str[i]==str1)
	  {
	  	k=1;
	  }	
	}
	if(k==1)
	cout<<str1<<" is a keyword";
	else
	cout<<str1<<" is a not keyword";
	
}
