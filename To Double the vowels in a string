// to double the vowels in a string
#include<conio.h>
#include<iostream>
#include<string.h>
using namespace std;
main()
{
	int i,j;
	char ch;
	string str,str1;
	cout<<"enter string";
	getline(cin,str);
	
	for(i=0;i<str.length();i++)
	{
	
		if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U')
		{
		    str1 +=str[i];
		}
     	str1 +=str[i];
     
	}
	cout<<str1;
}
